<h2>Available drones</h2>
<div>
    {{#each drones}}
    <div>
        <h3>Name: {{this.name}}</h3>
        <p>Propellers: {{this.propellers}}</p>
        <p>Max Speed: {{this.maxSpeed}}</p>
        <a href="/edit/{{_id}}">Edit drone</a>
        <form method="POST" action="/drones/{{_id}}/delete">
            <button type="submit">Delete</button>
        </form>
    </div>
    {{/each}}
</div>